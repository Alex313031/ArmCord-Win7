{
    "name": "GooseMod",
	"version": "1.0",
    "description": "Loads GooseMod, a discord client modification",
    "background": {
        "scripts": [
            "CSP.js"
        ]
    },
    "content_scripts": [
        {
          "matches": ["*://*.discord.com/*"],
          "js": ["gm.js"]
        }
      ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.discord.com/*"
    ],
    "manifest_version": 2
}